      "title": "Lifecycle(Mounting)",
      "items": [
        {
          "defination": "Before rendering",
          "code": "constructor (props)"
        }, {
          "defination": "Don’t use this",
          "code": "componentWillMount()"
        }, {
          "defination": "Render",
          "code": "render()"
        }, {
          "defination": "After rendering (DOM available)",
          "code": "componentDidMount()"
        }, {
          "defination": "Before DOM removal",
          "code": "componentWillUnmount()"
        }, {
          "defination": "Catch errors (16+) ",
          "code": "componentDidCatch()"
        }
      ]
    }, {
      "title": "Lifecycle(Updating)",
      "items": [
        {
          "defination": "Use setState() here",
          "code": "componentWillReceiveProps (newProps)"
        }, {
          "defination": "Skips render() if returns false",
          "code": "shouldComponentUpdate (newProps, newState)"
        }, {
          "defination": "Can’t use setState() here",
          "code": "componentWillUpdate (newProps, newState)"
        }, {
          "defination": "Render",
          "code": "render()"
        }, {
          "defination": "Operate on the DOM here",
          "code": "componentDidUpdate (prevProps, prevState)"
        }
      ]
